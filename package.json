{
  "name": "sequelize-cli",
  "version": "5.4.0",
  "description": "The Sequelize CLI",
  "bin": {
    "sequelize": "./src/sequelize"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "cli-color": "^1.4.0",
    "fs-extra": "^7.0.1",
    "js-beautify": "^1.8.8",
    "lodash": "^4.17.5",
    "umzug": "^2.1.0",
    "yargs": "^13.1.0"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "expect.js": "^0.3.1",
    "gulp": "^4.0.0",
    "mocha": "^6.0.0",
    "mysql2": "^1.6.1",
    "nodeify": "^1.0.1",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.x",
    "sqlite3": "^4.0.2",
    "through2": "^3.0.0"
  },
  "options": {
    "mocha": "--check-leaks --exit --timeout 30000 --colors --reporter spec"
  },
  "eslintIgnore": [
    "src/assets/seeders",
    "src/assets/models",
    "src/assets/migrations",
    "test/support/tmp/"
  ],
  "scripts": {
    "lint": "eslint \"test/**/*.js\" \"src/**/*.js\"",
    "test-raw": "mocha $npm_package_options_mocha 'test/**/*.test.js'",
    "test": "npm run lint && npm run test-raw"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sequelize/cli.git"
  },
  "keywords": [
    "sequelize",
    "cli"
  ],
  "contributors": [
    {
      "name": "Sascha Depold",
      "email": "sascha@depold.com"
    },
    {
      "name": "Paulo R Lopes",
      "email": "prplopes@gmail.com"
    },
    {
      "name": "Sushant Dhiman",
      "email": "sushantdhiman@outlook.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sequelize/cli/issues"
  },
  "homepage": "https://github.com/sequelize/cli",
  "engines": {
    "node": ">=8.0.0"
  }
}
